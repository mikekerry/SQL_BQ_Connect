import pyodbc
from google.cloud import bigquery
import pandas as pd

from pandasql import sqldf

connection = pyodbc.connect("Driver={ODBC Driver 13 for SQL Server};Server=10.36.111.117\OVOSQL03TST01,1433;Database=db_MIS_ETL;UID=xxx;PWD=xxx;")

sql = 'select Account_Signup_ID, Update_Audit_Key FROM [db_MIS_DW].[Core].[Fact_Account_Onboarding_Journey]'

df_sql = pd.read_sql(sql, connection)

#df_sql.to_csv('df_sql.csv')

bigquery_client = bigquery.Client.from_service_account_json('data-engineering-nonprod-mis-dev-2293f41e7fe8.json')

query = "SELECT Account_Signup_ID, Update_Audit_Key FROM  `data-engineering-156814.custom_mis.core_account_onboarding_journey`"

df_bq = pd.read_gbq(query, project_id='data-engineering-156814', private_key='data-engineering-nonprod-mis-dev-2293f41e7fe8.json', dialect='standard')

#df_sql.to_csv('df_bq.csv')

#print(df_sql.head())
#print(df_bq.head())

def pysqldf(q):
    return sqldf(q, globals())

q = """
    SELECT
        s.Account_Signup_ID
        , s.Update_Audit_Key
    FROM
        df_sql s
    LEFT JOIN
        df_bq b
            on s.Account_Signup_ID = b.Account_Signup_ID
            and s.Update_Audit_Key = b.Update_Audit_Key
    WHERE b.Account_Signup_ID IS NULL
    LIMIT 100;
    """

joined = pysqldf(q)
print(joined.head())
